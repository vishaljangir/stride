<p id="notice"><%= notice %></p>

<h1>Training Videos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @training_videos.each do |training_video| %>
      <tr>
        <td><%= link_to 'Show', training_video %></td>
        <td><%= link_to 'Edit', edit_training_video_path(training_video) %></td>
        <td><%= link_to 'Destroy', training_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Training Video', new_training_video_path %>
