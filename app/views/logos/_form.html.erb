<div class="wrapper ">
  <div class="main-panel">
    <div class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <div class="card">
              <div class="card-header card-header-primary">
                <h4 class="card-title">CHANGE LOGO</h4>
              </div>
              <div class="card-body">
                <%= form_with(model: logo, local: true) do |form| %>
                  <% if logo.errors.any? %>
                    <div id="error_explanation">
                      <ul>
                      <% logo.errors.full_messages.each do |message| %>
                        <li><%= message %></li>
                      <% end %>
                      </ul>
                    </div>
                  <% end %>
                  <div class="row">
                    <div class="col-md-2">
                      <label>Current logo</label>
                      <% if logo.present? %>
                        <%= image_tag(@logo.image, class: "img-thumbnail", alt: "Cinque Terre") %>
                      <% end %>
                    </div>
                    <div class="col-md-2">
                      <label>New logo</label>
                      <img id="blah" src="" />
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-12">
                      <br>
                      <span class="btn btn-default btn-file">
                        Browse <%= form.file_field :image, class: "form-control", id: "imgInp" %>
                      </span>
                      <br>
                      <br>
                      <%= form.label :contact %>
                      <%= form.number_field :contact, class: "form-control" %>
                      <%= form.label :email %>
                      <%= form.email_field :email, class: "form-control" %>
                       <%= form.label :location %>
                      <%= form.text_field :location, class: "form-control" %>
                      <%= form.label :about %>
                      <%= form.text_area :about, class: "form-control" %>
                    </div>
                  </div>
                  <%= form.submit "submit", class: "btn btn-primary pull-right"%>
                  <div class="clearfix"></div>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
