<div class="wrapper ">
    
    <div class="main-panel">
      <!-- Navbar -->
      
      <!-- End Navbar -->
      <div class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header card-header-primary">
                  <div class="row">
                    <div class="col-md-10">
                      <h4 class="card-title">LOGOS</h4>
                      <p class="card-category">Brand logos</p>
                    </div>
                    <div class="col-md-2">
                      <%= link_to 'Add Logo', new_logo_path, class: "btn btn-primary btn-sm" %>
                    </div>
                  </div>
                </div>
                <div class="card-body">
                    <div class="row">
                      <div class="col-md-12">
                        <div class="form-group">
                          <% @logos.each do |logo| %>
                            
                              <%= image_tag(logo.image, class: "img-thumbnail", alt: "Cinque Terre", width: "236", height: "236") %>
                              <!-- <td><%= link_to 'Show', logo %></td>
                              <td><%= link_to 'Edit', edit_logo_path(logo) %></td>
                              <td><%= link_to 'Destroy', logo, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
                            
                          <% end %>
                          
                        </div>
                      </div>
                    </div>
                    <div class="clearfix"></div>
                  
                </div>
              </div>
            </div>
           
          </div>
        </div>
      </div>
     
    </div>
  </div>
