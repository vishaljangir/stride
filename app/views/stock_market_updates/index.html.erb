<p id="notice"><%= notice %></p>

<h1>Stock Market Updates</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stock_market_updates.each do |stock_market_update| %>
      <tr>
        <td><%= stock_market_update.description %></td>
        <td><%= link_to 'Show', stock_market_update %></td>
        <td><%= link_to 'Edit', edit_stock_market_update_path(stock_market_update) %></td>
        <td><%= link_to 'Destroy', stock_market_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock Market Update', new_stock_market_update_path %>
